#include "internal.zh"
#include <str.zh>
#include <libc/stdlib.zh>
#include <libc/stdio.zh>

fn _libzr_crash_out(msg: Str) {
    let cmsg = cstr_from_str(msg);
    printf("libzr aborted: %s\n", cmsg);

    abort();
}

fn _libzr_crash_out_cstr(msg: *u8) {
    printf("libzr aborted: %s\n", msg);

    abort();
}